<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/list_albums">

    <fragment
        android:id="@+id/list_albums"
        android:name="com.example.examfebrepeated.features.album.presentation.AlbumListFragment"
        android:label="AlbumsList">
        <action
            android:id="@+id/action_list_albums_to_list_cards"
            app:destination="@id/list_cards"/>
    </fragment>
    <fragment
        android:id="@+id/list_cards"
        android:name="com.example.examfebrepeated.features.album.presentation.CardsListFragment">
        <argument
            android:name="albumId"
            app:argType="string" />
    </fragment>



</navigation>